{
  "name": "c-ufunc",
  "private": true,
  "version": "1.0.0",
  "description": "c-func is a set of Typescript and JavaScript functional methods",
  "keywords": [
    "javascript",
    "typescript",
    "functional programming"
  ],
  "repository": "git@github.com:otissv/ufunc.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "start": "npx http-server ./docs",
    "build": "npm run clean && mkdir build && ts-node node_modules/scripts/build-package",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "test": "jest",
    "test:coverage": "npm run build && jest --coverage",
    "test:watch": "jest --watch",
    "docs": "npm run build && typedoc src/index.ts",
    "clean": "rimraf ./build",
    "prepare": "npm run build",
    "publish": "cd build && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts"
    }
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "scripts": "workspace:*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jsdoc": "^3.6.10",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  }
}
