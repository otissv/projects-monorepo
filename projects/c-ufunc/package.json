{
  "name": "c-ufunc",
  "private": true,
  "version": "1.0.0",
  "description": "c-func is a set of Typescript and JavaScript functional methods",
  "keywords": [
    "javascript",
    "typescript",
    "functional programming"
  ],
  "repository": "git@github.com:otissv/ufunc.git",
  "author": "Otis Virginie",
  "license": "MIT",
  "main": "libs/index.ts",
  "types": "libs/index.ts",
  "scripts": {
    "start": "npx http-server ./docs",
    "build": "npm run clean && mkdir build && ts-node node_modules/scripts/libs/build-package",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "npm run build && jest --coverage",
    "test:watch": "jest --watch",
    "docs": "npm run build && typedoc src/index.ts",
    "clean": "rimraf ./build",
    "prepare": "npm run build",
    "publish": "cd build && npm publish",
    "purge": "rimraf node_modules"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint . --ext .ts"
    }
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "tsconfig": "workspace:*",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "scripts": "workspace:*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-functional": "^4.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jsdoc": "^3.6.11",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  }
}
