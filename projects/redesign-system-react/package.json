{
  "private": true,
  "name": "@redesign-system/react",
  "version": "0.1.0",
  "license": "MIT",
  "typings": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "run-p watch storybook",
    "build": "npm run clean && mkdir build && ts-node node_modules/scripts/build-package",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "npm run build && jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "lint:watch": "esw . --ext .ts -w",
    "fix": "eslint . --ext .ts  --fix",
    "storybook": "start-storybook -p 6007",
    "storybook:build": "build-storybook",
    "clean": "rm -rf ./build",
    "prepare": "npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "fluid": false
  },
  "author": "Otis Virginie",
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-docs": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addons": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/theming": "^6.5.9",
    "@types/facepaint": "^1.2.2",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.17.0",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-disable": "^2.0.3",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-watch": "^8.0.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "scripts": "workspace:*",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "tsconfig": "workspace:*",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@emotion/cache": "^11.9.3",
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.3",
    "@emotion/serialize": "^1.0.4",
    "@emotion/sheet": "^1.1.1",
    "@emotion/utils": "^1.1.0",
    "@redesign-system/theme": "workspace:*",
    "c-ufunc": "workspace:*",
    "csstype": "^3.1.0",
    "popmotion": "^11.0.3",
    "style-value-types": "^5.1.0"
  },
  "resolutions": {
    "@types/react": "^18.0.12"
  }
}
