{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "types/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "paths": {
    "*": [
      "types/*",
      "node_modules/*"
    ]
  },
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es5",
    "module": "esnext"
  }
}
